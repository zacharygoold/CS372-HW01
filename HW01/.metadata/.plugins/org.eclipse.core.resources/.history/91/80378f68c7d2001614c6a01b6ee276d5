

	public class Date{
		int month, day, year;
		
		public Date(int month, int day, int year){
			this.month = month;
			this.day = day;
			this.year = year;
		}
		//GET FUNCTIONS
		public int getMonth(){
			return month;
		}
		public int getDay(){
			return day;
		}
		public int getYear(){
			return year;
		}
		//SET FUNCTIONS
		public void setMonth(int month){
			this.month = month;
		}
		public void setDay(int day){
			this.day = day;
		}
		public void setYear(int year){
			this.year = year;
		}
		//display message
		public void message(){
			System.out.printf("\n\n"+month+"/"+day+"/"+year);
		}
	}

